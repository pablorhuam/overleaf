@article{Gorbachev448,
	Abstract = {Graphene has two distinct valleys in its electronic structure, in which the electrons have the same energy. Theorists have predicted that creating an asymmetry between the two valleys will coax graphene into exhibiting the so-called valley Hall effect (VHE). In this effect, electrons from the two valleys move across the sample in opposite directions when the experimenters run current along the sample. Gorbachev et al. achieved this asymmetry by aligning graphene with an underlying layer of hexagonalboron nitride (hBN) (see the Perspective by Lundeberg and Folk). The authors measured the transport characteristics of the sample, which were consistent with the theoretical predictions for the VHE. The method may in the future lead to information processing using graphene{\textquoteright}s valleys.Science, this issue p. 448; see also p. 422 Topological materials may exhibit Hall-like currents flowing transversely to the applied electric field even in the absence of a magnetic field. In graphene superlattices, which have broken inversion symmetry, topological currents originating from graphene{\textquoteright}s two valleys are predicted to flow in opposite directions and combine to produce long-range charge neutral flow. We observed this effect as a nonlocal voltage at zero magnetic field in a narrow energy range near Dirac points at distances as large as several micrometers away from the nominal current path. Locally, topological currents are comparable in strength with the applied current, indicating large valley-Hall angles. The long-range character of topological currents and their transistor-like control by means of gate voltage can be exploited for information processing based on valley degrees of freedom.},
	Author = {Gorbachev, R. V. and Song, J. C. W. and Yu, G. L. and Kretinin, A. V. and Withers, F. and Cao, Y. and Mishchenko, A. and Grigorieva, I. V. and Novoselov, K. S. and Levitov, L. S. and Geim, A. K.},
	Date-Added = {2018-10-30 10:49:20 -0200},
	Date-Modified = {2018-10-30 10:49:20 -0200},
	Doi = {10.1126/science.1254966},
	Eprint = {http://science.sciencemag.org/content/346/6208/448.full.pdf},
	Issn = {0036-8075},
	Journal = {Science},
	Number = {6208},
	Pages = {448--451},
	Publisher = {American Association for the Advancement of Science},
	Title = {Detecting topological currents in graphene superlattices},
	Url = {http://science.sciencemag.org/content/346/6208/448},
	Volume = {346},
	Year = {2014},
	Bdsk-Url-1 = {http://science.sciencemag.org/content/346/6208/448},
	Bdsk-Url-2 = {https://doi.org/10.1126/science.1254966}}


@article{Levy544,
	Abstract = {Recent theoretical proposals suggest that strain can be used to engineer graphene electronic states through the creation of a pseudo{\textendash}magnetic field. This effect is unique to graphene because of its massless Dirac fermion-like band structure and particular lattice symmetry (C3v). Here, we present experimental spectroscopic measurements by scanning tunneling microscopy of highly strained nanobubbles that form when graphene is grown on a platinum (111) surface. The nanobubbles exhibit Landau levels that form in the presence of strain-induced pseudo{\textendash}magnetic fields greater than 300 tesla. This demonstration of enormous pseudo{\textendash}magnetic fields opens the door to both the study of charge carriers in previously inaccessible high magnetic field regimes and deliberate mechanical control over electronic structure in graphene or so-called {\textquotedblleft}strain engineering.{\textquotedblright}},
	Author = {Levy, N. and Burke, S. A. and Meaker, K. L. and Panlasigui, M. and Zettl, A. and Guinea, F. and Neto, A. H. Castro and Crommie, M. F.},
	Date-Added = {2018-10-30 10:52:17 -0200},
	Date-Modified = {2018-10-30 10:52:17 -0200},
	Doi = {10.1126/science.1191700},
	Eprint = {http://science.sciencemag.org/content/329/5991/544.full.pdf},
	Issn = {0036-8075},
	Journal = {Science},
	Number = {5991},
	Pages = {544--547},
	Publisher = {American Association for the Advancement of Science},
	Title = {Strain-Induced Pseudo{\textendash}Magnetic Fields Greater Than 300 Tesla in Graphene Nanobubbles},
	Url = {http://science.sciencemag.org/content/329/5991/544},
	Volume = {329},
